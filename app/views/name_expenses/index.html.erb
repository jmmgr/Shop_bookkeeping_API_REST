<h1>Listing name_expenses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @name_expenses.each do |name_expense| %>
      <tr>
        <td><%= name_expense.name %></td>
        <td><%= link_to 'Show', name_expense %></td>
        <td><%= link_to 'Edit', edit_name_expense_path(name_expense) %></td>
        <td><%= link_to 'Destroy', name_expense, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Name expense', new_name_expense_path %>
